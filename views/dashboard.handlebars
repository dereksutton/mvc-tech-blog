<div class="container">
    <h1>Dashboard</h1>
    <button id="new-post-button">Add New Post</button>
    {{#each posts}}
        <div class="post">
            <h2><a href="/post/{{id}}">{{title}}</a></h2>
            <p>{{body}}</p>
            <button class="edit-button" data-id="{{id}}">Edit</button>
            <button class="delete-button" data-id="{{id}}">Delete</button>
        </div>
    {{/each}}
</div>